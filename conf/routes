GET    /                          controllers.HomeController.index(request: Request)
GET    /women                       controllers.HomeController.women(request: Request)
GET    /men                     controllers.HomeController.men
GET    /toplist                   controllers.HomeController.toplist

# Map static resources from the /public folder to the /assets URL path
GET    /assets/*file             controllers.Assets.versioned(path="/public", file: Asset)
GET    /favicon.ico              controllers.Assets.at(path="/public", file="images/cup.svg")